<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_reps_file" model="ir.cron">
        <field name="name">Send Files to REPS</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True"/>
        <field name="model_id" ref="model_reps_azure_file"/>
        <field name="code">model.cron_send_files()</field>
        <field name="state">code</field>
        <field name="active" eval="False"/>
    </record>
    
      <record id="ir_cron_file_generator" model="ir.cron">
        <field name="name">Generate Files</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True"/>
        <field name="model_id" ref="data_file_generator.model_file_generator_instance"/>
        <field name="code">model.cron_generate_files()</field>
        <field name="state">code</field>
        <field name="active" eval="False"/>
    </record>
</odoo>